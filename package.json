{
  "name": "pug",
  "private": true,
  "version": "2.4.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copyViteManifestForDev": "cp -r dist/.vite public",
    "build": "tsc && vite build && npm run copyViteManifestForDev",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup-portal-workflows": "mkdir -p .github/workflows && cp -r ./node_modules/decent-portal/github-workflows/*.yml .github/workflows",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.79",
    "@mlc-ai/web-runtime": "^0.18.0-dev2",
    "@mlc-ai/web-tokenizers": "^0.1.6",
    "decent-portal": "^2.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.24",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@webgpu/types": "^0.1.65",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "^3.2.4"
  }
}
